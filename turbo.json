{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        "cdk.out/**"
      ]
    },
    "deploy": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "lint": {},
    "format": {
      "dependsOn": [
        "lint"
      ]
    },
    "build-staging": {
      "dependsOn": [
        "antalmanac#build"
      ],
      "outputs": [
        "build/**",
        "cdk.out/**",
        "dist/**"
      ]
    },
    "deploy-staging": {
      "dependsOn": [
        "build-staging"
      ],
      "cache": false
    }
  }
}